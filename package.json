{
  "name": "@okta/samples-php-slim",
  "version": "0.0.1",
  "publishConfig": {
    "registry": "https://artifacts.aue1d.saasure.com/artifactory/api/npm/npm-okta"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okta/samples-php-slim.git"
  },
